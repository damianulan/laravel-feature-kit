{
  "name": "damianulan/laravel-feature-kit",
  "type": "library",
  "description": "Feature Kit for Laravel",
  "keywords": [
    "laravel",
    "config",
    "models",
    "policies",
    "kit",
    "feature"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Damian UÅ‚an",
      "email": "damian.ulan@protonmail.com",
      "homepage": "https://damianulan.me",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.3",
    "ext-json": "*",
    "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
    "mews/purifier": "^3.4"
  },
  "require-dev": {
    "mockery/mockery": "^1.5",
    "laravel/pint": "^1.0",
    "phpunit/phpunit": "^11.0|^12.0"
  },
  "autoload": {
    "psr-4": {
      "FeatureKit\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FeatureKit\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "post-update-cmd": [
      "./vendor/bin/pint"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "FeatureKit\\FeatureKitServiceProvider"
      ],
      "aliases": {
        "FormForge": "FeatureKit\\Facades\\FeatureKit"
      }
    }
  }
}
